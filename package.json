{
  "name": "pbjs",
  "version": "0.1.0",
  "private": true,
  "description": "Protobuf to javascript goog.proto2.Message compiler.",
  "dependencies": {
    "besync": "0.0.1",
    "funct": "0.0.2",
    "glob": "3.1.13",
    "mkdirp": "0.3.4",
    "pegjs": "0.7.0",
    "rimraf": "2.1.4",
    "soyset": "0.0.2"
  },
  "devDependencies": {
    "async": "*",
    "JSON2": "0.1.0",
    "mocha": "~1.8.1",
